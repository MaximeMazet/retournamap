{{ form_start(form) }}
<div class="row">
    <div class="col-md-6">{{ form_row(form.name) }}</div>
    <div class="col-md-6">{{ form_row(form.imageFile) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.delivery) }}</div>
    <div class="col-md-6">{{ form_row(form.label) }}</div>
    <div class="col-md-12">{{ form_row(form.contracts) }}</div>
</div>
{{ form_widget(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}
